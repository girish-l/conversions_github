{
    "name": "girish-l/conversions",
    "description": "This is my package conversions",
    "keywords": [
        "girish-l",
        "conversions"
    ],
    "homepage": "https://github.com/girish-l/conversions",
    "license": "MIT",
    "authors": [
        {
            "name": "Girish L",
            "email": "girish.l@hotmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Girishl\\Conversions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Girishl\\Conversions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
